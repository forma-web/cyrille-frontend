{
  "name": "cyrille-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "type:check": "tsc",
    "build:check": "vite build",
    "prettier:check": "prettier --check ./**/*.{js,jsx,ts,tsx,css,md,json}",
    "prettier:format": "prettier --loglevel warn --write ./**/*.{js,jsx,ts,tsx,css,md,json}",
    "lint:check": "eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.16",
    "@sentry/react": "^7.46.0",
    "@tanstack/react-query": "^4.26.1",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^3.1.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "fast-average-color": "^9.3.0",
    "html-react-parser": "^3.0.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.8.2",
    "sass": "^1.58.3",
    "typescript": "^4.9.3",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^2.4.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.1.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.4",
    "terser": "^5.16.8"
  },
  "browserslist": [
    "last 2 year",
    "not dead"
  ]
